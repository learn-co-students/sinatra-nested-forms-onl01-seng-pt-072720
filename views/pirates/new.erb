<h1>New Pirate!</h1>



<form action="/pirates" method="post">
  Pirate Name: <input type="text" name="pirate[name]">
  Pirate Weight: <input type="integer" name="pirate[weight]">
  Pirate Height: <input type="integer" name="pirate[height]">
  Pirate Ship Name: <input type="text" id= "ship_name_1" name="pirate[ships][][name]">
  Pirate Ship Type: <input type="text" id= "ship_type_1" name="pirate[ships][][type]">
  Pirate Ship Booty: <input type="text" id= "ship_booty_1" name="pirate[ships][][booty]">
  Pirate Ship 2 Name: <input type="text" id= "ship_name_2" name="pirate[ships][][name]">
  Pirate Ship 2 Type: <input type="text" id= "ship_type_2" name="pirate[ships][][type]">
  Pirate Ship 2 Booty: <input type="text" id= "ship_booty_2" name="pirate[ships][][booty]">
  <input type="submit" value="Submit" id="submit">
</form>
